{
  "name": "alibeez-to-calendar-sync",
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "dev": "nodemon --ignore data --require dotenv/config src/index.js",
    "start": "node src/index.js",
    "typecheck": "tsc",
    "prettier": "prettier *.json .circleci/config.yml src/**/*.js",
    "prettier:check": "npm run prettier -- --check",
    "prettier:write": "npm run prettier -- --write"
  },
  "devDependencies": {
    "@types/node": "14.6.1",
    "dotenv": "8.2.0",
    "nodemon": "2.0.6",
    "prettier": "2.1.1",
    "typescript": "4.0.2"
  }
}
